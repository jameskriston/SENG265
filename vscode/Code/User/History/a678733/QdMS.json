{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Survey with Input File",
      "type": "cppdbg",
      "request": "launch",
      "program": "C\\Windows\\system32\\cmd.exe",
      "args": [
        "/C",
        "${workspaceFolder}\\survey.exe < in03.txt"
      ],
      "cwd": "${workspaceFolder}",
      "externalConsole": true,
      "MIMode": "gdb",
      "miDebuggerPath": "C:\\MinGW\\bin\\gdb.exe",
      "preLaunchTask": "C/C++: gcc.exe build active file"
    }
  ]
}